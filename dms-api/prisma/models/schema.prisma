generator client {
  provider        = "prisma-client"
  output          = "../generated/prisma"
  moduleFormat    = "cjs"
  previewFeatures = ["typedSql"]
  binaryTargets   = ["native", "darwin-arm64", "linux-arm64-openssl-3.0.x", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
}
